{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^db:generate"],
      "outputs": [".next/**"]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "format:check": {
      "cache": false,
      "outputs": []
    },
    "format:write": {
      "cache": false,
      "outputs": []
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}

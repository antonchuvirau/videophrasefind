datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Video {
  indexId String
  videoId String

  title    String
  duration Decimal

  createdAt DateTime @default(now())

  userId String @unique
}

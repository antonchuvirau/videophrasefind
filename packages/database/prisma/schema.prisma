datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Video {
  id String @id @default(cuid())

  title        String
  duration     Decimal?
  size         Int?
  thumbnailUrl String?

  userId String?

  twelveLabsVideoId String?

  indexId   String?
  indexName String

  createdAt DateTime @default(now())
}
